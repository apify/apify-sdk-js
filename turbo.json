{
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".tshy",
                ".tshy-build"
            ]
        },
        "copy": {
            "dependsOn": [
                "^copy"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "clean": {
            "cache": false,
            "outputs": []
        }
    }
}
